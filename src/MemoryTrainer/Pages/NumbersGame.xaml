<UserControl x:Class="MemoryTrainer.Pages.NumbersGame"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MemoryTrainer.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Background="LightGreen" LastChildFill="True">

        <ToolBar Margin="5" Background="WhiteSmoke" DockPanel.Dock="Top">
            <Button x:Name="close" Command="{Binding Path=Close}" Background="AntiqueWhite">
                Close
            </Button>
        </ToolBar>

        <Grid Margin="5">
        </Grid>

    </DockPanel>
</UserControl>
